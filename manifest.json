{
  "manifest_version": 3,
  "name": "Backlog Mention Checker",
  "description": "Check Safety for Backlog Mentioned Comments.",
  "version": "1.0",
  "content_scripts": [{
    "matches": [
      "https://*.backlog.jp/view/*-*"
    ],
    "js": [
      "jquery-3.6.0.min.js",
      "check.js"
    ]
  }],
  "permissions": [
    "tabs"
  ]
}
